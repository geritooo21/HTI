<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.tue.demothermostat">

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ThermostatActivity"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ThermostatActivity"
            android:label="ThermostatActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WeekOverview"
            android:label="Week Overview"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".DayOverview"
            android:label="Monday"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".SetDayNight"
            android:label="Day/Night temperature"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

    </application>
ss
</manifest>