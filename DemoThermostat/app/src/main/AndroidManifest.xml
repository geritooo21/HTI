<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.tue.demothermostat">

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="Thermostat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ThermostatActivity"
            android:label="Thermostat Activity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WeekOverview"
            android:label="Week Overview"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".DayOverview"
            android:label="Day Overview"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".SetDayNight"
            android:label="Day/Night Temperature"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".SecretSettings"
            android:label="Secret Settings"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

    </application>
ss
</manifest>